  "description": "Making functions self destructing in a few seconds if not executed.",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/{**/,/}*.js",
    "coverage": "istanbul cover _mocha test/{**/,/}*.js -- -R spec",
